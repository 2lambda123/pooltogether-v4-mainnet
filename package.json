{
  "name": "@pooltogether/v4-testnet",
  "version": "1.0.0-beta.7",
  "main": "index.js",
  "repository": "git@github.com:pooltogether/v4-mainnet.git",
  "author": "Brendan Asselstine <mail.asselstine@gmail.com>",
  "license": "MIT",
  "scripts": {
    "addPrizes": "hardhat run ./scripts/addPrizes.js --network ",
    "clean": "rm -rf cache/ artifacts/",
    "compile": "hardhat compile",
    "task": "hardhat",
    "dashboard": "node tasks/dashboard",
    "docs": "hardhat deploy-markdown-export --name 'V4 Testnet' --npm-package-name @pooltogether/v4-testnet --github-base-url 'https://github.com/pooltogether/v4-testnet/tree/master' --output-filepath testnet.md",
    "verify-polygon": "hardhat --network polygon etherscan-verify --api-key $POLYGONSCAN_API_KEY",
    "verify-mainnet": "hardhat --network mainnet etherscan-verify --api-key $ETHERSCAN_API_KEY",
    "deploy-mainnet-to-polygon": "DEPLOY=polygon hardhat deploy --network polygon",
    "deploy-mainnet-to-ethereum": "DEPLOY=mainnet hardhat deploy --network mainnet",
    "deploy-goerli": "DEPLOY=goerli hardhat deploy --network goerli",
    "disburse": "hardhat run ./scripts/disburse.js --network ",
    "deposit": "hardhat run ./scripts/deposit.js --network ",
    "generate": "node ./scripts/generateContractList.js",
    "deploy-polygon": "DEPLOY=polygon hardhat deploy --network localhost",
    "deploy-mainnet": "DEPLOY=mainnet hardhat deploy --network localhost",
    "fork-polygon": "FORK_CHAIN_ID=$POLYGON_CHAIN_ID FORK_RPC_URL=$POLYGON_RPC_URL FORK_ENABLED=true DEPLOY=polygon hardhat node --no-deploy --network hardhat",
    "fork-mainnet": "FORK_CHAIN_ID=$MAINNET_CHAIN_ID FORK_RPC_URL=$MAINNET_RPC_URL FORK_ENABLED=true DEPLOY=mainnet hardhat node --no-deploy --network hardhat",
    "console": "hardhat console --network localhost",
    "node-goerli": "DEPLOY=goerli hardhat node",
    "prepare-deploy": "yarn clean && yarn compile",
    "prepack": "yarn generate",
    "reinstall": "rm -rf node_modules/ && yarn cache clean && yarn"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.3.2",
    "@pooltogether/aave-yield-source": "github://pooltogether/aave-yield-source#pool-1772-update-aave-yield-source-to-be-a",
    "@pooltogether/draw-calculator-js": "^1.0.0-beta.25",
    "@pooltogether/hardhat-deploy-markdown-export": "^0.1.0",
    "@pooltogether/owner-manager-contracts": "1.1.0",
    "@pooltogether/v4-autotask-lib": "github://pooltogether/v4-autotask-lib#master",
    "@pooltogether/v4-core": "1.0.0-beta.5",
    "@pooltogether/v4-periphery": "1.0.0-beta.7",
    "@pooltogether/v4-timelocks": "1.0.0-beta.4",
    "@pooltogether/yield-source-interface": "1.3.0",
    "find": "^0.3.0",
    "glob": "^7.1.7"
  },
  "devDependencies": {
    "@ethersproject/hardware-wallets": "^5.0.14",
    "ajv": "^6.12.2",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.10.1",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "debug": "^4.3.2",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.3",
    "hardhat-dependency-compiler": "^1.1.1",
    "hardhat-deploy": "^0.9.1",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-ethers": "^1.0.1",
    "idx": "^2.5.6",
    "luxon": "^2.0.2",
    "neo-blessed": "^0.2.0",
    "node-emoji": "^1.11.0",
    "node-pty": "^0.10.1",
    "prompt": "^1.2.0",
    "term.js": "^0.0.7"
  },
  "files": [
    "artifacts",
    "deployments/polygon",
    "deployments/mainnet",
    "testnet.json"
  ]
}
